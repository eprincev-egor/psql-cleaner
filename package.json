{
  "name": "psql-cleaner",
  "version": "0.0.10",
  "description": "removing unused joins and cte from select",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project publish-tsconfig.json",
    "test": "nyc --reporter=html --reporter=text-summary mocha",
    "lint": "tslint lib/**/*.ts && tslint test/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eprincev-egor/psql-cleaner.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eprincev-egor/psql-cleaner/issues"
  },
  "homepage": "https://github.com/eprincev-egor/psql-cleaner#readme",
  "dependencies": {
    "grapeql-lang": "^2.9.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "exclude": [
      "coverage",
      ".mocharc.js"
    ]
  },
  "devDependencies": {
    "@types/mocha": "^8.0.2",
    "@types/node": "^12.12.6",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
